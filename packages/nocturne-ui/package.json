{
  "name": "nocturne-ui",
  "version": "0.0.1-alpha.0",
  "description": "一个vue3组件库",
  "keywords": [
    "nocturne-ui",
    "component library",
    "ui",
    "vue"
  ],
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@popperjs/core": "^2.11.8",
    "@types/lodash": "^4.17.7",
    "@vueuse/core": "^9.13.0",
    "async-validator": "^4.2.5",
    "dayjs": "^1.11.11",
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "vue": "^3.4.15",
    "vue-router": "^4.0.16"
  },
  "main": "lib/index.js",
  "module": "es/index.mjs",
  "types": "es/index.d.ts",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.mjs",
      "require": "./lib/index.js"
    },
    "./es/*": {
      "types": [
        "./es/*.d.ts",
        "./es/*/index.d.ts"
      ],
      "import": "./es/*.mjs"
    },
    "./es": {
      "types": "./es/index.d.ts",
      "import": "./es/index.mjs"
    },
    "./es/*.mjs": {
      "types": "./es/*.d.ts",
      "import": "./es/*.mjs"
    },
    "./lib/*": {
      "types": [
        "./lib/*.d.ts",
        "./lib/*/index.d.ts"
      ],
      "require": "./lib/*.js"
    },
    "./lib": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js"
    },
    "./lib/*.js": {
      "types": "./lib/*.d.ts",
      "require": "./lib/*.js"
    },
    "./*": "./*",
    "./global": {
      "types": "./global.d.ts"
    }
  },
  "sideEffect": [
    "dist/*",
    "theme-chalk/src/**/*.scss",
    "theme-chalk/src/*.css"
  ],
  "browserslist": [
    "> 1%",
    "not ie 11"
  ]
}